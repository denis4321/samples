<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE log4j:configuration SYSTEM "log4j.dtd"><log4j:configuration xmlns:log4j="http://jakarta.apache.org/log4j/">    <appender name="feed" class="org.apache.log4j.RollingFileAppender">        <param name="Name" value="file-appender"/>        <param name="File" value="../logs/feed.log"/>        <param name="MaxFileSize" value="5MB"/>        <param name="MaxBackupIndex" value="50"/>        <param name="Append" value="true"/>        <layout class="org.apache.log4j.PatternLayout">            <param name="ConversionPattern" value="[%d{dd-MMM-yy:HH:mm:ss,SSS}] {%c{1}} (%x)> %m%n"/>        </layout>    </appender>    <appender name="error-feed" class="org.apache.log4j.RollingFileAppender">        <param name="Threshold" value="ERROR"/>        <param name="Name" value="file-appender"/>        <param name="File" value="../logs/error-feed.log"/>        <param name="MaxFileSize" value="5MB"/>        <param name="MaxBackupIndex" value="50"/>        <param name="Append" value="true"/>        <layout class="org.apache.log4j.PatternLayout">            <param name="ConversionPattern" value="[%d{dd-MMM-yy:HH:mm:ss}] {%c{1}} (%x)> %m%n"/>        </layout>    </appender>    			    <logger name="feed-logger" additivity="false">        <level value="INFO"/>        <appender-ref ref="feed"/>        <appender-ref ref="error-feed"/>    </logger>           <root>        <priority value="ERROR"/>        <appender-ref ref="error-feed"/>    </root></log4j:configuration>